{
  "name": "vislet",
  "version": "0.0.1",
  "private": true,
  "description": "Small interactive visualizations",
  "homepage": "https://www.vislet.com",
  "repository": "https://github.com/zamiang/vislet",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Brennan Moore",
    "email": "brennanmoore@gmail.com",
    "url": "https://www.zamiang.com"
  },
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "audit-dependencies": "npm audit --audit-level=high --production",
    "build": "next build",
    "dev": "next -p ${PORT:-3000}",
    "lint": "npx eslint './**/*.{js,ts,tsx}'",
    "lint:fix": "npx eslint --fix './**/*.{js,ts,tsx}'",
    "prettier": "prettier --write \"{pages,components}/**/*.{ts,tsx,js,css}\"",
    "prettier:check": "prettier --list-different \"{pages,components}/**/*.{ts,tsx,js,css}\"",
    "start": "next start -p ${PORT:-3000}",
    "typecheck": "node node_modules/.bin/tsc --project tsconfig.json --noEmit --pretty"
  },
  "dependencies": {
    "@types/express": "4.17.13",
    "@types/react-headroom": "2.2.2",
    "@types/react-linkify": "1.0.1",
    "@types/react-router-dom": "5.3.3",
    "axios": "0.27.2",
    "clsx": "1.2.0",
    "express": "4.18.1",
    "next": "12.2.0",
    "next-compose-plugins": "2.2.1",
    "next-fonts": "1.5.1",
    "next-secure-headers": "2.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "17.4.0",
    "sitemap": "7.1.1",
    "slugify": "1.6.5",
    "typescript": "4.7.4"
  },
  "devDependencies": {
    "@types/node": "18.0.1",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.30.4",
    "@typescript-eslint/eslint-plugin-tslint": "5.30.4",
    "@typescript-eslint/parser": "5.30.4",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "1.5.0",
    "prettier": "2.7.1",
    "tslint": "6.1.3"
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}
