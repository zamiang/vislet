#three
  .heading
    margin-bottom: -8px

#three-svg
  border: 1px solid $border-color
  background: url('/images/new-york-small.jpg')
  background-size: cover
  height: 600px
  width: 500px
  @media screen and (max-width: 500px)
    width: 340px
    height: 400px

.three-svg-key
  transform: rotate(90deg)
  position: absolute
  top: 266px
  left: -347px

.commercial-sales
  display: none

.svg-building-class, .svg-line-graph
  height: 270px
  width: 600px
  vertical-align: top
  display: inline-block
  @media screen and (max-width: 500px)
    width: 340px
  &.third-width
    width: 700px
    @media screen and (max-width: 500px)
      width: 340px

.sales-group
  margin-top: 20px

.svg-slider
  margin-top: -13px
  margin-left: -14px
.select-container
  top: -37px
  left: -1px

.map-app .svg-container
  width: 105%

.graph-keys
  column-count: 4
  height: 143px
