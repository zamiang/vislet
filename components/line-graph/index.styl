.svg-line-graph
  font-size: 12px
  fill: #333

  .axis path, .axis line
    fill: none
    stroke: #333
    shape-rendering: crispEdges

  .x-axis path
    display: none

  .line
    fill: none
    stroke: steelblue
    stroke-width: 1.5px
    &.mean-line
      stroke: lightgrey
    &.compare-line
      stroke: hsl(353, 64%, 54%)

  circle
    &.mean-circle
      stroke: lightgrey

