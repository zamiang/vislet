.tract
  fill: rgba(250, 250, 250, 0.2)
  stroke: rgba(0, 0, 0, 0.2)
  transition fill 0.2s

.tract:hover
  fill: rgba(70, 130, 180, 0.2)
  
.tract.selected
  fill: rgba(70, 130, 180, 0.8)

.subunit-label 
  fill: #777
  fill-opacity: .5
  font-size: 2px
  text-anchor: middle
  pointer-events: none
  franklin-gothic()

.park
  fill: #eee
  stroke: #777
