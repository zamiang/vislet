.tract
  fill: rgb(250, 250, 250)
  stroke: #777
  transition fill 0.3s

.tract:hover
  fill: #E3F3FF
  
.tract.selected
  fill: rgb(196, 226, 250)
  .subunit-label 
    fill: #ccc

.subunit-label 
  fill: #777
  fill-opacity: .5
  font-size: 2px
  text-anchor: middle
  pointer-events: none
  franklin-gothic()

.park
  fill: #eee
  stroke: #777
